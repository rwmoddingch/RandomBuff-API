{
  "metadata": [
    {
      "src": [
        {
          "src": "src/**",
          "files": [
            "**/RandomBuff.dll",
            "**/RandomBuffUtils.dll"
          ]
        }
        
      ],
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "dest": "docs/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "RandomBuff-API",
      "_appTitle": "RandomBuff-API",
      "_enableSearch": true,
      "pdf": false
    }
  }
}